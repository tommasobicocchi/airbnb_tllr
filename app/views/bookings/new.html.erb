<div class="container">
<%= simple_form_for [@flat, @booking] do |f| %>
  <%= f.input :checkin_date, as: :string, required: false, input_html: {class: "datepicker"} %>
  <%= f.input :checkout_date, as: :string, required: false, input_html: {class: "datepicker"} %>
  <%= f.button :submit %>
<% end %>
</div>
